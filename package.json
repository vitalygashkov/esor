{
  "name": "esor",
  "version": "0.0.6",
  "author": "Vitaly Gashkov <vitalygashkov@vk.com>",
  "description": "Fast download utility for Node.js and standalone use",
  "license": "AGPL-3.0",
  "keywords": [
    "downloader",
    "lightweight",
    "http",
    "https",
    "curl",
    "wget"
  ],
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitalygashkov/esor.git"
  },
  "bugs": {
    "url": "https://github.com/vitalygashkov/esor/issues",
    "email": "vitalygashkov@vk.com"
  },
  "funding": {
    "type": "individual",
    "url": "https://boosty.to/vitalygashkov"
  },
  "main": "esor.js",
  "types": "./types/esor.d.ts",
  "bin": {
    "esor": "esor.js"
  },
  "scripts": {
    "build": "npm run build:linux && npm run build:mac && npm run build:mac:arm && npm run build:win",
    "build:win": "pkg . -t node20-win-x64 -o ./build/win-x64/esor.exe",
    "build:mac": "pkg . -t node20-macos-x64 -o ./build/macos-x64/esor",
    "build:mac:arm": "pkg . -t node20-macos-arm64 -o ./build/macos-arm64/esor",
    "build:linux": "pkg . -t node20-linux-x64 -o ./build/linux-x64/esor",
    "build:bun": "bun build . --compile --outfile esor",
    "test:mp4": "node esor https://cdn.bitmovin.com/content/assets/art-of-motion-dash-hls-progressive/MI201109210084_mpeg-4_hd_high_1080p25_10mbits.mp4",
    "test:dash": "node esor https://dash.akamaized.net/dash264/TestCases/1a/sony/SNE_DASH_SD_CASE1A_REVISED.mpd",
    "test:hls": "node esor https://multiplatform-f.akamaihd.net/i/multi/will/bunny/big_buck_bunny_,640x360_400,640x360_700,640x360_1000,950x540_1500,.f4v.csmil/master.m3u8"
  },
  "engines": {
    "node": ">=20.0"
  },
  "dependencies": {
    "fastq": "^1.17.1",
    "undici": "^6.10.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@yao-pkg/pkg": "^5.11.5",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3"
  }
}
